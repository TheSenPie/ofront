/* registers must be stored on the stack before doing garbage collection on the stack.
i386 version.
*/
static void SYSTEM_callMarkStack(void) {
	asm ("push %eax");
	asm ("push %ebx");
	asm ("push %ecx");
	asm ("push %edx");
	asm ("push %esi");
	asm ("push %edi");
	SYSTEM_MarkStack();
	asm ("pop %edi");
	asm ("pop %esi");
	asm ("pop %edx");
	asm ("pop %ecx");
	asm ("pop %ebx");
	asm ("pop %eax");
}
